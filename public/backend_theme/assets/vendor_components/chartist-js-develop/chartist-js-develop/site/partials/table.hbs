{{#if button}}<a href="#" class="button" data-toggle-visible="#{{id}}">{{button}}</a>{{/if}}
<table id="{{id}}" role="presentation">
  {{#if headerBlade}}
    <thead>
      <tr>
      {{#each headerBlade}}
        <th>{{this}}</th>
      {{/each}}
      </tr>
    </thead>
  {{/if}}

  <tbody>
    {{#each rows}}
      <tr>
        {{#each this}}
          <td>{{{this}}}</td>
        {{/each}}
      </tr>
    {{/each}}
  </tbody>
</table>
